{
  "schemaVersion": 1,
  "id": "socksproxyclient",
  "version": "${mod_version}",
  "name": "SocksProxyClient",
  "description": "Explicitly use SOCKS proxy to connect to Minecraft servers.",
  "authors": [
    "CrimsonEdgeHope"
  ],
  "license": "WTFPL",
  "contact": {
    "homepage": "https://github.com/CrimsonEdgeHope/SocksProxyClientOfMinecraft",
    "sources": "https://github.com/CrimsonEdgeHope/SocksProxyClientOfMinecraft",
    "issues": "https://github.com/CrimsonEdgeHope/SocksProxyClientOfMinecraft/issues"
  },
  "icon": "assets/socksproxyclient/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "crimsonedgehope.minecraft.fabric.socksproxyclient.SocksProxyClient"
    ],
    "modmenu": [
      "crimsonedgehope.minecraft.fabric.socksproxyclient.modmenu.ModMenuEntry"
    ]
  },
  "mixins": [
    "socksproxyclient.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "java": ">=17",
    "fabricloader": ">=${fabricloader_version}"
  },
  "recommends": {
    "fabric": "*",
    "cloth-config2": "^${cloth_config_version}"
  },
  "suggests": {
    "modmenu": "^${modmenu_version}"
  },
  "conflicts": {
    "fastipping": "*"
  },
  "breaks": {
    "viafabric": ">=0.4.6",
    "proxyserver": "*"
  }
}